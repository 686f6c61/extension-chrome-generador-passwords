{
    "manifest_version": 3,
    "name": "Generador de Contraseñas Seguras",
    "version": "1.0",
    "description": "Genera contraseñas seguras fácilmente.",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": [
      "clipboardWrite"
    ]
  }